@model IEnumerable<TGProyectoG.Data.Carrera>

@{
    ViewBag.Title = "Indice";
}

<h2>Indice</h2>

<p>
    @Html.ActionLink("Crear nuevo", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NombreCarrera)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NombreCarrera)
        </td>
        <td>
            @Html.ActionLink("Modificar", "Edit", new { id=item.IdCarrera }) |
            @*@Html.ActionLink("Details", "Details", new { id=item.IdCarrera })*@ |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.IdCarrera })
        </td>
    </tr>
}

</table>
